algorithm no.8

enum enPassFail { Pass = 1, Fail = 2 };

int ReadMark()
{
	int Mark;
	cout << "Pls enter the mark : ";
	cin >> Mark;
	return Mark;
}

static enPassFail checkMark(int Mark)
{
	if (Mark >= 50)
		return enPassFail::Pass;
	else
		return enPassFail::Fail;
}

static void PrintMark(int Mark)
{
	if (checkMark(Mark) == enPassFail::Pass)
		cout << "You Passed\n";
	else
		cout << "you Failed\n";
}

int main()
{
	PrintMark(ReadMark());
	return 0;
}
